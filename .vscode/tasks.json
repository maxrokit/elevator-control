{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-api",
      "type": "shell",
      "command": "dotnet build ${workspaceFolder}/src/Api/Api.csproj -c Debug",
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "install-client",
      "type": "shell",
      "command": "npm install",
      "options": {
        "cwd": "${workspaceFolder}/src/Client"
      },
      "problemMatcher": []
    }
  ]
}
